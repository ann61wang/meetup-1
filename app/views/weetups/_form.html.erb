<%= simple_form_for @weetup do |f| %>
  <div class="form-weetup">
    <%= f.input :title, input_html: { class: "form-control"} %>
    <%= f.input :description, input_html: { class: "form-control"} %>
  </div>
  <%= f.submit "确定", class: "btn btn-primary", data: { disable_with: "Submiting..." } %>
<% end %>
